package sec04_exam02;

public class KeyCodeExample {

	public static void main(String[] args) throws Exception {
		int keyCode;
		
		keyCode = System.in.read();
		System.out.println("keyCode: " + keyCode);
		
		keyCode = System.in.read();
		System.out.println("keyCode: " + keyCode);
		
		keyCode = System.in.read();
		System.out.println("keyCode: " + keyCode);

	}

}

// a
// keyCode: 97
// keyCode: 13 (엔터임ㅎㅎ)
// keyCode: 10 (엔터임ㅎㅎ)